                                        <script><![CDATA[import flash.events.Event;
import flash.ui.Keyboard;
import flash.events.KeyboardEvent;
import flash.events.MouseEvent;

var up:Boolean = false;
var down:Boolean = false;
var left:Boolean = false;
var right:Boolean = false;

var nadpis1 = new win1();
var nadpis2 = new win2();

var goal1 = 0;
var goal2 = 0;
schet.text = goal1 + " : " + goal2;

var soundOn = true;

var board = {};
board.clip = board_mc;
board.speed = 3;
board.w = board.clip.width;
board.h = board.clip.height;

var pc = {};
pc.clip = board_mc2;
pc.speed = 3;
pc.w = pc.clip.width;
pc.h = pc.clip.height;

var ball = {};
ball.clip = ball_mc;
ball.speed = 3;
ball.radius = ball.clip.width/2;
ball.xmov = ball.speed;
ball.ymov = 2 + Math.random()*2;
ball.live = true;

stage.addEventListener(KeyboardEvent.KEY_DOWN, kd);
stage.addEventListener(KeyboardEvent.KEY_UP, ku);
stage.addEventListener(Event.ENTER_FRAME, ef);
povtor.addEventListener(MouseEvent.CLICK, repeat);
knopka.addEventListener(MouseEvent.CLICK, mc);

function mc (e:MouseEvent){
	if(soundOn){
		soundOn = false;
		knopka.gotoAndStop(2);
	}else{
		soundOn = true;
		knopka.gotoAndStop(1);
	}
}


function playSound(zvuk){
		if(soundOn){
			musicBox.gotoAndStop(zvuk);
		}
}
function repeat (e:MouseEvent){
	if(goal1 == 5){
		removeChild(nadpis1);
	}
	if(goal2 == 5){
		removeChild(nadpis2);
	}
	ball.clip.x = 300;
	ball.clip.y = 200;
	ball.xmov = 3;
	ball.ymov = 2 + Math.random()*2;
	ball.clip.visible = true;
	ball.live = true;
	goal1 = 0;
	goal2 = 0;
	g1.gotoAndStop(1);
	g2.gotoAndStop(1);
	schet.text = goal1 + " : " + goal2;
}
function kd(e:KeyboardEvent){
	trace(e);//Узнаем какой код у кнопки вниз
	if(e.keyCode ==87){
		up=true;
	}
	if(e.keyCode==83){
		down=true;
	}
}
function ku(e:KeyboardEvent){
	trace(e);//Узнаем какой код у кнопки вниз
	if(e.keyCode ==87){
		up=false;
	}
	if(e.keyCode==83){
		down=false;
	}
}

//Алгоритм движения доски
function board_player(){
	if(up){
		if((board.clip.y - board.h/2)> wall_up.y){
			board.clip.y -= board.speed;
			if(board.clip.y - board.h/2 < wall_up.y){
				board.clip.y = wall_up.y + board.h/2;
			}
		}
	}
	if(down){
		if((board.clip.y + board.h/2) < wall_down.y){
		board.clip.y += board.speed;
		if(board.clip.y + board.h/2 >  wall_down.y){
			board.clip.y = wall_down.y - board.h/2;
			}
			
		}
	}
}
//Алгоритм движения доски противника
function board_pc(){
	if(ball.clip.y < pc.clip.y){
		if((pc.clip.y - pc.h/2)> wall_up.y){
			pc.clip.y -= pc.speed;
			if(pc.clip.y - pc.h/2 < wall_up.y){
				pc.clip.y = wall_up.y + pc.h/2;
			}
		}
	}
	if(ball.clip.y > pc.clip.y){
		if((pc.clip.y + pc.h/2) < wall_down.y){
		pc.clip.y += pc.speed;
		if(pc.clip.y + pc.h/2 >  wall_down.y){
			pc.clip.y = wall_down.y - pc.h/2;
			}
		}
	}
}
//временное положение мячика
function moveBalls(){
	ball.tempx = ball.clip.x + ball.xmov;
	ball.tempy = ball.clip.y + ball.ymov;
}
//Определяем столкновение шира с полом и потолком
function detection(){
	//Столкновение с верхней границей
	if(ball.tempy - ball.radius < wall_up.y){
		var yc = wall_up.y + ball.radius;
		var otn = (ball.tempy - yc)/(ball.tempy - ball.clip.y);
		var xc = ball.tempx - otn*(ball.tempx - ball.clip.x);
		ball.tempx = xc;
		ball.tempy = yc;
		ball.ymov *= -1;
		playSound("stena");
	}
	//столкновение с нижней границей
	if(ball.tempy + ball.radius > wall_down.y){
		yc = wall_down.y - ball.radius;
		otn = (ball.tempy - yc)/(ball.tempy - ball.clip.y);
		xc = ball.tempx - otn*(ball.tempx - ball.clip.x);
		ball.tempx = xc;
		ball.tempy = yc;
		ball.ymov *= -1;
		playSound("stena");
	}
}
function board_ball_detect () {
	//столкновение с доской игрока
	if(ball.tempx - ball.radius < board.clip.x + board.w/2){
		if((ball.clip.y <= board.clip.y + board.h/2 + ball.radius/2)
			&& (ball.clip.y >= board.clip.y - board.h/2 - ball.radius/2)
		&& ball.live){
			var xc = board.clip.x + board.w/2 + ball.radius;
			var otn = (ball.tempx - xc)/(ball.tempx - ball.clip.x);
			var yc = ball.tempy - otn*(ball.tempy - ball.clip.y);
			ball.tempx = xc;
			ball.tempy = yc;
			ball.xmov *= -1;
			ball.xmov += 0.5;
			ball.ymov *= 1.1;
			pc.speed *= 1.09;
			board.speed *= 1.16;
			playSound("igrok");
		}else{
			ball.live = false;
		}
	}
	//столкновение с доской противника
	if(ball.tempx + ball.radius >= pc.clip.x - pc.w/2){
		if((ball.clip.y <= pc.clip.y + pc.h/2 + ball.radius/2)
		&& (ball.clip.y >= pc.clip.y - pc.h/2 - ball.radius/2)
		&& ball.live){
			xc = pc.clip.x - pc.w/2 - ball.radius;
			otn = (ball.tempx - xc)/(ball.tempx - ball.clip.x);
			yc = ball.tempy - otn*(ball.tempy - ball.clip.y);
			ball.tempx = xc;
			ball.tempy = yc;
			ball.xmov *= -1;
			ball.xmov -= 0.5;
			ball.ymov *= 1.1;
			pc.speed *= 1.09;
			board.speed *= 0.99;
			playSound("sopernik");
		}else{
			ball.live = false;
		}
	}
}
//рисуем шар
function render(){
	ball.clip.x = ball.tempx;
	ball.clip.y = ball.tempy;
}
//когда забит гол
function goal(){
	if(ball.clip.x < -20){
		goal2 += 1;
		ball.clip.x = 300;
		ball.clip.y = 200;
		ball.live = true;
		ball.xmov = 3;
		ball.ymov = 2 + Math.random()*2;
		schet.text = goal1 + " : " + goal2;
		g2.gotoAndStop(goal2+1);
		pc.speed = 3;
		board.speed = 3;
	}
	if(ball.clip.x > 620){
		goal1 += 1;
		ball.clip.x = 300;
		ball.clip.y = 200;
		ball.live = true;
		ball.xmov = 3;
		ball.ymov = 2 + Math.random()*2;
		schet.text = goal1 + " : " + goal2;
		g1.gotoAndStop(goal1+1);
		pc.speed = 3;
		board.speed = 3;
	}
	if(goal1 == 5){
		ball.xmov = 0;
		ball.ymov = 0;
		ball.clip.visible = false;
		addChild(nadpis1);
		nadpis1.x = 300;
		nadpis1.y = 200;
	}
	if(goal2 == 5){
		ball.xmov = 0;
		ball.ymov = 0;
		ball.clip.visible = false;
		addChild(nadpis2);
		nadpis2.x = 300;
		nadpis2.y = 200;

	}
}
function ef(e:Event){
	moveBalls();
	board_player();
	board_pc();
	detection();
	board_ball_detect ();
	render();
	goal();
	
	
}]]></script>
